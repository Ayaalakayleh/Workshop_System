@using Workshop.Resources
@using Microsoft.Extensions.Localization
@inject IStringLocalizer<Common> Common
@inject IStringLocalizer<Messages> Message

<div class="tab-pane fade" id="tabOperators" role="tabpanel">
    <div class="card">
        <div class="card-body">
            <div class="row g-3">
                <div class="col-lg-4"><label class="form-label">@Common["Label_CreatingOperator"]</label><input class="form-control" id="OpCreating" value="@ViewBag.CreatingOperator" readonly></div>
                <div class="col-lg-4"><label class="form-label">@Common["Label_InvoicingOperator"]</label><input class="form-control" id="OpInvoicing" value="@ViewBag.InvoicingOperator" readonly></div>
                <div class="col-lg-4"><label class="form-label">@Common["Label_BookedOutOperator"]</label><input class="form-control" id="OpBookedOut" value="@ViewBag.BookedOutOperator" readonly></div>

            </div>
        </div>
    </div>
</div>