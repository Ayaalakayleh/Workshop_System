<script>
    function switchLanguage(culture) {
        Swal.fire({
            title: culture === 'ar' ? 'جاري تغيير اللغة...' : 'Changing Language...',
            text: culture === 'ar' ? 'يرجى الانتظار' : 'Please wait...',
            allowOutsideClick: false,
            didOpen: () => Swal.showLoading()
        });

        $.ajax({
            url: '@Url.Action("SetLanguage", "Language")',
            type: 'POST',
            data: {
                culture: culture,
                returnUrl: window.location.pathname + window.location.search
            },
            success: function () {
                location.reload();
            },
            error: function () {
                Swal.fire({
                    icon: 'error',
                    title: culture === 'ar' ? 'خطأ!' : 'Error!',
                    text: culture === 'ar' ? 'حدث خطأ أثناء تغيير اللغة' : 'An error occurred while changing the language',
                    confirmButtonText: culture === 'ar' ? 'حسناً' : 'OK'
                });
            }
        });
    }

    // safer: serialize as real JSON so quotes/newlines won't break your JS
    const resources = @Html.Raw(System.Text.Json.JsonSerializer.Serialize(new {
        select = CommonLocalizer["Select"].Value,
        description = CommonLocalizer["Description"].Value,
        drag_drop_files = CommonLocalizer["DragDropFiles"].Value,
        EmptyData = CommonLocalizer["EmptyData"].Value,
        areYouSure = MessagesLocalizer["SwalDeleteTitle"].Value,
        yes = MessagesLocalizer["Yes"].Value,
        no = MessagesLocalizer["No"].Value,
        success = MessagesLocalizer["Success"].Value,
        failed = MessagesLocalizer["Failed"].Value,
        all_required_fields = MessagesLocalizer["AllRequiredFields"].Value,
        required_field = MessagesLocalizer["RequiredField"].Value,
        valid_email = MessagesLocalizer["ValidEmail"].Value,
        error = MessagesLocalizer["Error"].Value,
        error_msg = MessagesLocalizer["ErrorMessage"].Value,
        success_msg = MessagesLocalizer["SuccessMessage"].Value,
        deleted_success = MessagesLocalizer["DeletedSuccess"].Value,
        ok = MessagesLocalizer["Ok"].Value,
        letters_numbers = MessagesLocalizer["LettersNumbers"].Value,
        already_exists = MessagesLocalizer["AlreadyExists"].Value,
        delete_not_allowed = MessagesLocalizer["DeleteNotAllowed"].Value,
        NoDataInTable = CommonLocalizer["NoDataInTable"].Value,
        filepondDragDrop = CommonLocalizer["filepondDragDrop"].Value,
    }));

    $(document).ready(function () {
        const currentCulture = document.documentElement.lang;
        document.documentElement.dir = currentCulture === 'ar' ? 'rtl' : 'ltr';

        $(".link-item a").on("click", function () {
            const targetUrl = $(this).attr("href");
            if (targetUrl && targetUrl !== "#") window.location.href = targetUrl;
        });

        $(".plate-mask").inputmask("9999 - A A A", {
            definitions: {
                'A': { validator: "[A-Za-z]", casing: "upper" }
            },
            placeholder: "_"
        });
    });
</script>

<script>
    (() => {
        const themeBase = '@Url.Content("~/smartadmin-5-styles/css/themes/")';
        const themeLink = document.getElementById('theme-style');

        const storageKey = 'sa-theme-style';
        const defaultTheme = 'earth';

        function applyTheme(theme) {
            const t = (theme || defaultTheme).toLowerCase();
            themeLink.href = `${themeBase}${t}.css`;
            localStorage.setItem(storageKey, t);

            document.querySelectorAll('input[data-action="theme-style"]').forEach(r => {
                r.checked = ((r.dataset.themeStyle || '').toLowerCase() === t);
            });
        }

        applyTheme(localStorage.getItem(storageKey));

        document.addEventListener('change', (e) => {
            const el = e.target;
            if (el && el.matches('input[data-action="theme-style"]')) {
                e.stopImmediatePropagation();
                applyTheme(el.dataset.themeStyle);
            }
        }, true);
    })();
</script>

